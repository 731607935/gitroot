<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="WeChatMessage">
    <typeAlias alias="weChatMessage" type="com.tom.msg.vo.WchatMessageVo"/>
    <resultMap id="WeChatMessageMap" class="weChatMessage">
        <result property="id" column="id"/>
        <result property="fromuser" column="fromuser"/>
        <result property="touser" column="touser"/>
        <result property="subject" column="subject"/>
        <result property="message_id" column="message_id"/>
    </resultMap>

    
    <select id="getMessageList" resultMap="WeChatMessageMap">
    	select * from wechat_message
    </select>
    
    <delete id="delMessage" parameterClass="long">
    	delete from wechat_message where id = #id#
    </delete>
</sqlMap>